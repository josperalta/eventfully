<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/eventfully/settings/wcm/templates/page-content"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="eventfully/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="eventfully/components/container"
            layout="responsiveGrid">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="eventfully/components/container"
                layout="responsiveGrid">
                <title
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="eventfully/components/title"/>
                <text
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="eventfully/components/text"/>
                <image
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="eventfully/components/image"/>
                <button
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="eventfully/components/button"
                    jcr:title="Register"/>
                <rsvp
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="eventfully/components/rsvp"/>
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="eventfully/components/container"
                    layout="responsiveGrid"/>
            </container>
        </root>
    </jcr:content>
</jcr:root>
